---
import IndexStartingSearch from "@/components/indexStartingSearch.astro";
import SectionContainer from "@/components/sectionContainer.astro";
import Layout from "@/layouts/Layout.astro";
---

<Layout
title="404: Not Found"
description="This page could not be found"
headerNavActive="none"
>
    <SectionContainer className="!mb-0 !mt-0 !py-28">
        <div class="flex flex-col gap-5 items-center">
            <h1 class="text-[3rem] pb-56 pt-40 font-extrabold text-purple-400 animate-pulse">
                Page not found :(
            </h1>
        </div>
        <div class="flex flex-col w-full items-center">
            <IndexStartingSearch className="!mb-0" />
        </div>
    </SectionContainer>
</Layout>